{
  "manifest_version": 2,
  "name": "XC Ski Strava",
  "version": "2.0.0",
  "description": "XC Ski Strava extension",
  "icons": { 
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png" 
  },
  "web_accessible_resources": [
    "snowflake.png"
  ],
  "content_scripts": [
    {
      "matches": ["https://www.strava.com/dashboard"],
      "css": ["/static/css/main.css"],
      "js": ["/static/js/main.js"]
    }
  ]
}
